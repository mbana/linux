# add-auto-load-safe-path /usr/lib/golang/src/runtime/runtime-gdb.py
add-auto-load-safe-path /usr/share/go-1.18/src/runtime/runtime-gdb.py
set debuginfod enabled on
set disassembly-flavor intel
set history filename ~/.gdb_history
set history save
set history save on
set pagination off
set print pretty on
set confirm off


